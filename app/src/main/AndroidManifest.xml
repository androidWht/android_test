<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.asus4.android_test">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".jni.JniActivity" />
        <activity android:name=".shader.ShaderActivity" />
        <activity android:name=".constraintlayout.ConstraintLayoutActivity" />
        <activity
            android:name=".webvideo.WebVideoActivity"
            android:configChanges="orientation|keyboard|screenSize|keyboardHidden" />
        <activity android:name=".progressbar.ProgressBarActivity" />
        <activity android:name=".paint.PaintActivity"></activity>
    </application>

</manifest>